{
  "name": "glean-developer-site-revamp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --host 127.0.0.1 --port 8888",
    "build": "pnpm run generate:changelog && pnpm run generate:rss && docusaurus build",
    "build:fast": "GENERATE_API_DOCS=false pnpm run generate:changelog && pnpm run generate:rss && docusaurus build",
    "start:fast": "GENERATE_API_DOCS=false docusaurus start --host 127.0.0.1 --port 8889",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "changelog:new": "node scripts/create-changelog-entry.mjs",
    "generate:changelog": "node scripts/generate-changelog-data.mjs",
    "generate:rss": "node scripts/generate-rss-feed.mjs",
    "generate:redirects": "tsx scripts/generate-redirects.ts",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "format:check": "prettier --check .",
    "openapi:regenerate:all": "pnpm run openapi:clean:before:all && pnpm run openapi:transform:all && pnpm run openapi:generate:all && pnpm run openapi:clean:after:all",
    "openapi:regenerate:client": "pnpm run openapi:clean:before:client && pnpm run openapi:transform:client && pnpm run openapi:generate:client && pnpm run openapi:clean:after:client",
    "openapi:regenerate:indexing": "pnpm run openapi:clean:before:indexing && pnpm run openapi:transform:indexing && pnpm run openapi:generate:indexing && pnpm run openapi:clean:after:indexing",
    "openapi:clean:before:all": "pnpm run openapi:clean:before:client && pnpm run openapi:clean:before:indexing",
    "openapi:clean:before:client": "find docs/api/client-api -type f ! -name 'overview.mdx' -delete",
    "openapi:clean:before:indexing": "find docs/api/indexing-api -type f ! -name '*-overview.mdx' -delete",
    "openapi:transform:all": "pnpm run openapi:transform:client && pnpm run openapi:transform:indexing",
    "openapi:transform:client": "pnpm run openapi:capitalize:client && pnpm run openapi:break:client",
    "openapi:capitalize:client": "node scripts/openapi-capitalize-language.mjs https://gleanwork.github.io/open-api/specs/final/client_rest.yaml openapi/client/client-capitalized.yaml",
    "openapi:break:client": "node scripts/openapi-split-break-circular.mjs openapi/client/client-capitalized.yaml openapi/client/split-apis",
    "openapi:transform:indexing": "node scripts/openapi-capitalize-language.mjs https://gleanwork.github.io/open-api/specs/final/indexing.yaml openapi/indexing/indexing-capitalized.yaml",
    "openapi:generate:all": "pnpm run openapi:generate:client && pnpm run openapi:generate:indexing",
    "openapi:generate:client": "npm-run-all \"openapi:generate:client:*\"",
    "openapi:generate:client:activity": "docusaurus gen-api-docs activity",
    "openapi:generate:client:announcements": "docusaurus gen-api-docs announcements",
    "openapi:generate:client:answers": "docusaurus gen-api-docs answers",
    "openapi:generate:client:authentication": "docusaurus gen-api-docs authentication",
    "openapi:generate:client:chat": "docusaurus gen-api-docs chat",
    "openapi:generate:client:agents": "docusaurus gen-api-docs agents",
    "openapi:generate:client:collections": "docusaurus gen-api-docs collections",
    "openapi:generate:client:documents": "docusaurus gen-api-docs documents",
    "openapi:generate:client:insights": "docusaurus gen-api-docs insights",
    "openapi:generate:client:messages": "docusaurus gen-api-docs messages",
    "openapi:generate:client:pins": "docusaurus gen-api-docs pins",
    "openapi:generate:client:search": "docusaurus gen-api-docs search",
    "openapi:generate:client:entities": "docusaurus gen-api-docs entities",
    "openapi:generate:client:shortcuts": "docusaurus gen-api-docs shortcuts",
    "openapi:generate:client:summarize": "docusaurus gen-api-docs summarize",
    "openapi:generate:client:verification": "docusaurus gen-api-docs verification",
    "openapi:generate:client:tools": "docusaurus gen-api-docs tools",
    "openapi:generate:client:governance": "docusaurus gen-api-docs governance",
    "openapi:generate:indexing": "docusaurus gen-api-docs indexing",
    "openapi:clean:after:all": "pnpm run openapi:clean:after:client && pnpm run openapi:clean:after:indexing",
    "openapi:clean:after:client": "find docs/api/client-api -type f -name 'sidebar.ts' -delete && find docs/api/client-api -type f -name '*.info.mdx' -delete && find docs/api/client-api -type f -name '*.tag.mdx' -delete",
    "openapi:clean:after:indexing": "rm -f docs/api/indexing-api/sidebar.ts docs/api/indexing-api/glean-api.info.mdx && find docs/api/indexing-api -type f -name '*.info.mdx' -delete && find docs/api/indexing-api -type f -name '*.tag.mdx' -delete"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/plugin-client-redirects": "^3.8.1",
    "@docusaurus/plugin-content-docs": "^3.8.1",
    "@docusaurus/plugin-vercel-analytics": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-classic": "^3.8.1",
    "@docusaurus/theme-common": "^3.8.1",
    "@docusaurus/theme-search-algolia": "^3.8.1",
    "@docusaurus/utils": "^3.8.1",
    "@docusaurus/utils-validation": "^3.8.1",
    "@gleanwork/mcp-config-schema": "^0.12.1",
    "@mdx-js/react": "^3.0.0",
    "@signalwire/docusaurus-plugin-llms-txt": "^1.1.0",
    "@vercel/edge-config": "^1.1.0",
    "allof-merge": "^0.6.7",
    "clsx": "^2.1.1",
    "docusaurus-plugin-openapi-docs": "^4.4.0",
    "docusaurus-plugin-search-glean": "^0.7.0",
    "docusaurus-theme-openapi-docs": "^4.4.0",
    "feed": "^5.1.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.513.0",
    "marked": "^15.0.12",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-feather": "^2.0.10",
    "react-tooltip": "^5.29.1",
    "sonner": "^2.0.7",
    "swiper": "^11.2.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@docusaurus/faster": "^3.8.1",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/xml2js": "^0.4.14",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/ui": "^3.2.4",
    "chalk": "^5.4.1",
    "fast-levenshtein": "^3.0.0",
    "inquirer": "^12.6.3",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "open": "^10.1.2",
    "prettier": "^3.6.1",
    "sass": "^1.89.1",
    "sass-loader": "^16.0.5",
    "slugify": "^1.6.6",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "vitest": "^3.2.4",
    "xml2js": "^0.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": "22.x"
  },
  "packageManager": "pnpm@10.13.1"
}
