{
  "name": "changelog-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "changelog-generator": "build/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "run": "pnpm run build && node build/index.js run",
    "analyze": "pnpm run build && node build/index.js analyze",
    "apply": "pnpm run build && node build/index.js apply"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "debug": "^4.3.6",
    "find-up": "^8.0.0",
    "js-yaml": "^4.1.0",
    "octokit": "^4.1.4",
    "@gleanwork/api-client": "^0.13.0"
  },
  "devDependencies": {
    "@octokit/types": "^15.0.0",
    "@types/debug": "^4.1.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.6.2",
    "typescript": "~5.9.3"
  }
}
